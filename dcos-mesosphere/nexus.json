{
  "labels": {
    "DCOS_PACKAGE_OPTIONS": "",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_SERVICE_NAME": "nexus",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_VERSION": "3.5.0-02",
    "DCOS_PACKAGE_NAME": "nexus",
    "MARATHON_SINGLE_INSTANCE_APP": "true"
  },
  "id": "/nexus",
  "acceptedResourceRoles": [
    "*"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "constraints": [
    [
      "hostname",
      "IS",
      "10.10.0.9"
    ]
  ],
  "container": {
    "portMappings": [
      {
        "containerPort": 8081,
        "hostPort": 12126,
        "protocol": "tcp",
        "servicePort": 10002
      }
    ],
    "type": "MESOS",
    "volumes": [
      {
        "containerPath": "nexus-data",
        "hostPath": "/tmp",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "sonatype/nexus3:3.5.0",
      "forcePullImage": false,
      "parameters": []
    }
  },
  "cpus": 0.5,
  "disk": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 5,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "COMMAND",
      "command": {
        "value": "curl --user admin:admin123 --fail --request GET ${HOST}:${PORT0}/service/metrics/ping"
      }
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 1600,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": []
}